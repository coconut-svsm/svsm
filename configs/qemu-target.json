{
    "igvm": {
        "qemu": {
            "output": "coconut-qemu.igvm",
            "platforms": [
                "snp",
                "tdp"
            ],
            "policy": "0x30000",
            "measure": "print",
            "check-kvm": true
        }
    },
    "kernel": {
        "svsm": {
            "features": "vtpm",
            "binary": true
        },
        "stage2": {
            "manifest": "kernel/Cargo.toml",
            "binary": true,
            "objcopy": "binary"
        },
        "tdx-stage1": {
            "type": "make",
            "output_file": "bin/stage1-trampoline",
            "objcopy": "binary"
        }
    },
    "firmware": {
        "env": "FW_FILE"
    }
}
